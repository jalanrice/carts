function text_setup()
	texts={}	
	add_text(7,0,"there are 4 keys in this map!\ngood luck getting them!\nit's filled with traps!")
end

function add_text(x,y,message)
	texts[x+y*128]=message
end

function get_text(x,y)
	return texts[x+y*128]
end

function draw_text()
	if (active_text)	then
		textx=mapx*8+4
		texty=mapy*8+48
		
		rectfill(textx,texty,textx+119,texty+31,0)
		print(active_text, textx+4, texty+4,7)
		print("ğŸ…¾ï¸ to close",textx+4, texty+23,8)
	end
		
		if (btnp(ğŸ…¾ï¸)) active_text=nil
end
